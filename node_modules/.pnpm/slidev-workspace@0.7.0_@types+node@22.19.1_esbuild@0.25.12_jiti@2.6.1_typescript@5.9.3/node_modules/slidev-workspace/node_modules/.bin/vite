#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/luggs/Projekte/GBSSG/slides/node_modules/.pnpm/rolldown-vite@7.2.10_@types+node@22.19.1_esbuild@0.25.12_jiti@2.6.1_yaml@2.8.2/node_modules/rolldown-vite/bin/node_modules:/Users/luggs/Projekte/GBSSG/slides/node_modules/.pnpm/rolldown-vite@7.2.10_@types+node@22.19.1_esbuild@0.25.12_jiti@2.6.1_yaml@2.8.2/node_modules/rolldown-vite/node_modules:/Users/luggs/Projekte/GBSSG/slides/node_modules/.pnpm/rolldown-vite@7.2.10_@types+node@22.19.1_esbuild@0.25.12_jiti@2.6.1_yaml@2.8.2/node_modules:/Users/luggs/Projekte/GBSSG/slides/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/luggs/Projekte/GBSSG/slides/node_modules/.pnpm/rolldown-vite@7.2.10_@types+node@22.19.1_esbuild@0.25.12_jiti@2.6.1_yaml@2.8.2/node_modules/rolldown-vite/bin/node_modules:/Users/luggs/Projekte/GBSSG/slides/node_modules/.pnpm/rolldown-vite@7.2.10_@types+node@22.19.1_esbuild@0.25.12_jiti@2.6.1_yaml@2.8.2/node_modules/rolldown-vite/node_modules:/Users/luggs/Projekte/GBSSG/slides/node_modules/.pnpm/rolldown-vite@7.2.10_@types+node@22.19.1_esbuild@0.25.12_jiti@2.6.1_yaml@2.8.2/node_modules:/Users/luggs/Projekte/GBSSG/slides/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../rolldown-vite@7.2.10_@types+node@22.19.1_esbuild@0.25.12_jiti@2.6.1_yaml@2.8.2/node_modules/rolldown-vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../../../../../rolldown-vite@7.2.10_@types+node@22.19.1_esbuild@0.25.12_jiti@2.6.1_yaml@2.8.2/node_modules/rolldown-vite/bin/vite.js" "$@"
fi
