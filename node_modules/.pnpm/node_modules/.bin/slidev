#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/luggs/Projekte/GBSSG/slides/node_modules/.pnpm/@slidev+cli@52.10.1_@babel+parser@7.28.5_@nuxt+kit@3.20.1_@types+markdown-it@14.1.2_@ty_017444d6dc6e2b6d37a0d57d7ee3a2ff/node_modules/@slidev/cli/bin/node_modules:/Users/luggs/Projekte/GBSSG/slides/node_modules/.pnpm/@slidev+cli@52.10.1_@babel+parser@7.28.5_@nuxt+kit@3.20.1_@types+markdown-it@14.1.2_@ty_017444d6dc6e2b6d37a0d57d7ee3a2ff/node_modules/@slidev/cli/node_modules:/Users/luggs/Projekte/GBSSG/slides/node_modules/.pnpm/@slidev+cli@52.10.1_@babel+parser@7.28.5_@nuxt+kit@3.20.1_@types+markdown-it@14.1.2_@ty_017444d6dc6e2b6d37a0d57d7ee3a2ff/node_modules/@slidev/node_modules:/Users/luggs/Projekte/GBSSG/slides/node_modules/.pnpm/@slidev+cli@52.10.1_@babel+parser@7.28.5_@nuxt+kit@3.20.1_@types+markdown-it@14.1.2_@ty_017444d6dc6e2b6d37a0d57d7ee3a2ff/node_modules:/Users/luggs/Projekte/GBSSG/slides/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/luggs/Projekte/GBSSG/slides/node_modules/.pnpm/@slidev+cli@52.10.1_@babel+parser@7.28.5_@nuxt+kit@3.20.1_@types+markdown-it@14.1.2_@ty_017444d6dc6e2b6d37a0d57d7ee3a2ff/node_modules/@slidev/cli/bin/node_modules:/Users/luggs/Projekte/GBSSG/slides/node_modules/.pnpm/@slidev+cli@52.10.1_@babel+parser@7.28.5_@nuxt+kit@3.20.1_@types+markdown-it@14.1.2_@ty_017444d6dc6e2b6d37a0d57d7ee3a2ff/node_modules/@slidev/cli/node_modules:/Users/luggs/Projekte/GBSSG/slides/node_modules/.pnpm/@slidev+cli@52.10.1_@babel+parser@7.28.5_@nuxt+kit@3.20.1_@types+markdown-it@14.1.2_@ty_017444d6dc6e2b6d37a0d57d7ee3a2ff/node_modules/@slidev/node_modules:/Users/luggs/Projekte/GBSSG/slides/node_modules/.pnpm/@slidev+cli@52.10.1_@babel+parser@7.28.5_@nuxt+kit@3.20.1_@types+markdown-it@14.1.2_@ty_017444d6dc6e2b6d37a0d57d7ee3a2ff/node_modules:/Users/luggs/Projekte/GBSSG/slides/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@slidev/cli/bin/slidev.mjs" "$@"
else
  exec node  "$basedir/../@slidev/cli/bin/slidev.mjs" "$@"
fi
